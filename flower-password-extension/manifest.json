{
  "manifest_version": 3,
  "name": "花密 Flower Password",
  "version": "1.0",
  "description": "花密 (Flower Password) 浏览器插件版 - 自动生成并填充可记忆的强密码",
  "permissions": [
    "activeTab",
    "storage",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "host_permissions": [
    "<all_urls>"
  ]
}